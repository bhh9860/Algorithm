package Package0806;

public class File01_OverlapLoop {
	public static void main(String[] args) {
		for (int n = 1; n <= 9; n++) {
			System.out.println("======%dë‹¨======", n+1);
			for (int dan = 2; dan <= 9; dan++) {
				System.out.printf("%d * %d = %d ", dan, n, dan*n);
			}
			System.out.println();
		}
	}
}
