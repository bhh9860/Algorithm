package Package0806;

public class File01_OverlapLoop {
	public static void main(String[] args) {
		for (int i = 2; i <= 9; i++) {
			System.out.println(i+"ë‹¨");
		}
		
		for (int n = 1; n <= 9; n++) {
			for (int dan = 2; dan <= 9; dan++) {
				System.out.printf("%d * %d = %2d\t", dan, n, dan*n);
			}
			System.out.println();
		}
	}
}
