package Package0804;

import java.util.Scanner;

public class Exmaple02_Coor {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		int d = input.nextInt();
		
		int first = a-c;
		int second = b-d;
		
//		if (first > 0) System.out.println(first);
//		else if (first < 0)	System.out.println(first);
//		else System.out.println(second);
		
		if (first == 0) System.out.println(second);
		else System.out.println(first);
		
		input.close();
		
	}
}
