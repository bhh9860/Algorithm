package Package0804;

import java.util.Scanner;

public class Example03_minus {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int num = input.nextInt();
		while (num >= 0) {
			num = input.nextInt();
		}
		System.out.println(num);
			
		input.close();
	}
}
