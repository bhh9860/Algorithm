package Package0805;

import java.util.Scanner;

public class Example01_for {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		
		int multi = a * b* c;
		int sum = 0;
		
		for (int i = 1; multi > i; i *= 10) {
			System.out.println(i);
			//			if (multi / i % 10 == 2) {
//				sum += 1;
			}
		}
	}
