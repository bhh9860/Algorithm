package Package0806;

public class File01_OverlapLoop {
	public static void main(String[] args) {
		int name = 0;
		
		for (int n = 1; n <= 9; n++) {
			System.out.printf("%dë‹¨",name++);
			for (int dan = 2; dan <= 9; dan++) {
				System.out.printf("%d * %d = %2d\t", dan, n, dan*n);
			}
			System.out.println();
		}
	}
}
