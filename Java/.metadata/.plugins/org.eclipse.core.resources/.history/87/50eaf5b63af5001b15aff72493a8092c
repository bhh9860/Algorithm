package Package0805;

import java.util.Scanner;

public class Homework {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int num = input.nextInt();
		int count = 10;
		int sum = 0;
		
		while (num > count) {
			sum += num % count / (count/10);
			count *= 10;
		}
		
		
		input.close();
		
	}
}
