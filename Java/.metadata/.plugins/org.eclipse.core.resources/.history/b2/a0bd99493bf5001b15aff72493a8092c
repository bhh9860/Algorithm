package Package0805;

import java.util.Scanner;

public class Homework {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int num = input.nextInt();
		int count = 1;
		int sum = 0;
		
		while (num > count) {
			count *= 10;
			sum += num % count / (count/10);
		}
		System.out.printf(sum + " " + count);
		
		input.close();
		
	}
}
