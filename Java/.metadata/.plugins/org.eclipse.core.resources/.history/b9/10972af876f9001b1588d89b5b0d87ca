package Package0806;

import java.util.Scanner;

public class File01_Array {
	public static void main(String[] args) {
		int [] arr1 = new int[5];
		

		//참보 변수의 선언과 동시에 초기화를 하면
		//new int []를 생략할 수 있다.
		
		int [] arr3 = {1, 2, 3, 4};
		
		System.out.println(new int[5]);
		Scanner input = new Scanner(System.in);
		
		for (int i = 0; i < 5; i++) {
			int a = input.nextInt();
			int [] arr4 = new int[a];
			System.out.println(arr4[i]);
		}
		
		
	}
}
