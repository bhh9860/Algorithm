import java.util.Scanner;

public class Exmaple04_Sum {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int n = 0;
		int sum = 0;
		int a=1, b=2, c=7;
		while (n >= 0) {
			sum += n;
			n = input.nextInt();
		}
		
		System.out.println(sum);
		
		input.close();
		
	}
}
